  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "allowJs": true,
    "experimentalDecorators": true,
